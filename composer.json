{
  "name": "falseclock/dbd-php-entity",
  "type": "library",
  "description": "Entities annotation for PHP Database driver",
  "keywords": [
    "dbd",
    "entity",
    "primitive",
    "database",
    "orm",
    "annotation",
    "view",
    "table",
    "constraint",
    "mapping"
  ],
  "license": "Apache-2.0",
  "authors": [
    {
      "name": "Nurlan Mukhanov",
      "email": "nurike@gmail.com"
    }
  ],
  "require": {
    "php": "^7.1|^8.0",
    "ext-json": "*",
    "myclabs/php-enum": "^1.7.7",
    "falseclock/dbd-php-common": "dev-master"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5.0",
    "php-coveralls/php-coveralls": "^2.4.3"
  },
  "autoload": {
    "psr-4": {
      "DBD\\Entity\\": "src/DBD/Entity/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "DBD\\Entity\\Tests\\": "tests/DBD/Entity/Tests/"
    },
    "files": [
      "tests/DBD/Entity/Tests/Entities/PersonBase.php",
      "tests/DBD/Entity/Tests/Entities/Address.php",
      "tests/DBD/Entity/Tests/Entities/Embedded/Street.php",
      "tests/DBD/Entity/Tests/Entities/Embedded/StreetWithZipCode.php",
      "tests/DBD/Entity/Tests/Entities/Embedded/ZipCode.php",
      "tests/DBD/Entity/Tests/Entities/Embedded/Region.php"
    ]
  }
}
